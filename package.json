  "eggPlugin": {
    "name": "firebase"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "firebase"
  ],
  "dependencies": {
    "firebase-admin": "^9.2.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
